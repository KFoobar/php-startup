{
    "name": "kfoobar/php-startup",
    "type": "project",
    "description": "Bolierplater for PHP projects.",
    "keywords": [
        "boilerplate",
        "kfoobar",
        "startup",
        "project"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "guzzlehttp/guzzle": "^6.3",
        "philo/laravel-blade": "^3.1",
        "vlucas/phpdotenv": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "David Villa",
            "email": "david@kfoobar.se"
        }
    ]
}
